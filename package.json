  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@react-native-community/bob": "^0.17.1",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "babel-loader": "^8.2.2",
    "typescript": "~3.9.5",
    "typedoc": "0.16.10"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "files": [
    "dist",
    "!**/__tests__",
